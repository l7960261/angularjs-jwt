{
  "name": "angularjs-jwt",
  "version": "0.0.2",
  "description": "Library to help you work with JWT accessToken/refreshToken on AngularJS",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run build:dist && karma start karma-src.conf.js --single-run",
    "lint": "eslint src test",
    "build": "npm run build:dist && npm run build:lib",
    "build:dist": "webpack --config ./webpack.config.dist.js --mode=production",
    "build:lib": "webpack --config ./webpack.config.js --mode=production",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l7960261/angularjs-jwt.git"
  },
  "keywords": [
    "AngularJS",
    "JWT"
  ],
  "author": "Wilson Weng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/l7960261/angularjs-jwt/issues"
  },
  "homepage": "https://github.com/l7960261/angularjs-jwt#readme",
  "devDependencies": {
    "angular": "^1.7.8",
    "angular-mocks": "^1.7.8",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "karma": "^4.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^6.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  },
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15"
  }
}
